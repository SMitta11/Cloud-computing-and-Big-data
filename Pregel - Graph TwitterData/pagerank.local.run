#!/bin/bash
#SBATCH -A uot189
#SBATCH --job-name="pagerank.local"
#SBATCH --output="pagerank.local.out"
#SBATCH --partition=shared
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=4
